    "def calculate_val(expression_label, intensity):\n",
    "    valence = 0\n",
    "    #if expression_label != '0': print (expression_label, intensity)\n",
    "    #using range 'rule of thumb' to derive stand. dev. for normal dist.\n",
    "    # (max range - min range)/ 4\n",
    "    \n",
    "    lbl1_mean = .5\n",
    "    lbl1_range = .2 #range is mean plus or minus this number\n",
    "    lbl2_mean = -.5\n",
    "    lbl2_range = .25 \n",
    "    lbl3_mean = -.7\n",
    "    lbl3_range = .15 \n",
    "    lbl4_mean = -.2\n",
    "    lbl4_range = .2 \n",
    "    lbl5_mean = -.6\n",
    "    lbl5_range = .2 \n",
    "\n",
    "    lbl1_sd = ((lbl1_mean+lbl1_range)-(lbl1_mean-lbl1_range))/4\n",
    "    lbl2_sd = ((lbl2_mean+lbl2_range)-(lbl2_mean-lbl2_range))/4\n",
    "    lbl3_sd = ((lbl3_mean+lbl3_range)-(lbl3_mean-lbl3_range))/4\n",
    "    lbl4_sd = ((lbl4_mean+lbl4_range)-(lbl4_mean-lbl4_range))/4\n",
    "    lbl5_sd = ((lbl5_mean+lbl5_range)-(lbl5_mean-lbl5_range))/4\n",
    "\n",
    "    if expression_label == '1': #aha\n",
    "        if intensity == -1:  #less intensity\n",
    "            valence = random.gauss(lbl1_mean-.1,lbl1_sd)\n",
    "        elif intensity == 1: #more intensity\n",
    "            valence = .5 + random.gauss(lbl1_mean+.1,lbl1_sd)\n",
    "        else: #regular intensity\n",
    "            valence = random.gauss(lbl1_mean,lbl1_sd)\n",
    "    elif expression_label == '2': #anxious\n",
    "        if intensity == -1: \n",
    "            valence = -.5 + random.uniform(-.1, .3)\n",
    "        elif intensity == 1: \n",
    "            valence = -.5 + random.uniform(-.3, .1)\n",
    "        else: \n",
    "            valence = random.gauss(lbl2_mean,lbl2_sd)\n",
    "    elif expression_label == '3': #confused\n",
    "        if intensity == -1: \n",
    "            valence = -.7 + random.uniform(-.05, .2)\n",
    "        elif intensity == 1: \n",
    "            valence = -.7 + random.uniform(-.2, .1)\n",
    "        else: \n",
    "            valence = random.gauss(lbl3_mean,lbl3_sd)\n",
    "    elif expression_label == '4': #distracted\n",
    "        if intensity == -1: \n",
    "            valence = -.2 + random.uniform(-.4, .25)\n",
    "        elif intensity == 1: \n",
    "            valence = -.2 + random.uniform(-.25, .1)\n",
    "        else: \n",
    "            valence = random.gauss(lbl4_mean,lbl4_sd)\n",
    "    elif expression_label == '5': #frustrated\n",
    "        if intensity == -1: \n",
    "            valence = -.6 + random.uniform(-.05, .2)\n",
    "        elif intensity == 1: \n",
    "            valence = -.6 + random.uniform(-.35, -.05)\n",
    "        else: \n",
    "            valence = random.gauss(lbl5_mean,lbl5_sd)\n",
    "    elif expression_label == '0': #unclassified\n",
    "        valence = -2 #0 + random.uniform(-.1, .1)\n",
    "    else:\n",
    "        # default\n",
    "        valence = 0\n",
    "    return valence"
   ]
